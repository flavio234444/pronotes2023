{
    "presets" : [
        [
            "@babel/preset-env",{
                // Los targets son las plataformas donde se ejecutara el codigo final
                "targets" : { "node" : "current"}
            }
        ],
        [
            "minify",{
                "builtIns" : false,
                "evaluate" : false,
                "mangle" : false
            }
        ]
    ],
    "plugins": [
        ["module-resolver", {
            "root":["./"],
            "alias": {
                "@server" : "./server"
            }
        }]
    ]
}